{
  "schemaVersion": 1,
  "id": "galacticraft-energy",
  "version": "${version}",

  "name": "Galacticraft Energy",
  "description": "An energy API that utilizes LibBlockAttributes.",

  "contact": {
    "homepage": "https://galacticraft.team",
    "sources": "https://github.com/TeamGalacticraft/Galacticraft-Energy",
    "issues": "https://github.com/TeamGalacticraft/Galacticraft-Energy/issues"
  },

  "authors": [
    {
      "name": "Team Galacticraft",
      "contact": {
        "homepage": "https://github.com/TeamGalacticraft",
        "discord": "https://discord.gg/n3QqhMYyFK"
      }
    }
  ],

  "contributors": [
    {
      "name": "marcus8448",
      "contact": {
        "homepage": "https://github.com/marcus8448"
      }
    },
    {
      "name": "JoeZwet",
      "contact": {
        "homepage": "https://github.com/JoeZwet"
      }
    }
  ],

  "license": "MIT",
  "icon": "assets/galacticraft-energy/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "dev.galacticraft.energy.GalacticraftEnergy"
    ]
  },
  "custom": {
    "waila:plugins": {
      "id": "galacticraft-energy:plugin",
      "initializer": "dev.galacticraft.energy.compat.waila.GalacticraftEnergyWailaPlugin"
    }
  },
  "mixins": [
    "galacticraft-energy.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.10.0",
    "minecraft": ">=1.16",
    "libblockattributes_core": ">=0.8"
  },
  "suggests": {
    "waila": ">=2"
  }
}
